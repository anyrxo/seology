{
  "name": "seology-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postbuild": "node prisma/init-db.js || echo 'Init DB skipped'",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && next build && node prisma/init-db.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:seed": "tsx prisma/seed.ts",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/reset.ts && npm run db:seed",
    "db:reset-only": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/reset.ts",
    "db:backup": "bash scripts/backup-db.sh",
    "db:restore": "bash scripts/restore-db.sh"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@clerk/nextjs": "^6.34.1",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.13.1",
    "bull": "^4.16.5",
    "canvas-confetti": "^1.9.4",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.552.0",
    "next": "^14.2.25",
    "next-swagger-doc": "^0.4.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.27.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^4.3.2",
    "recharts": "^3.3.0",
    "redis": "^5.9.0",
    "resend": "^6.4.0",
    "sonner": "^2.0.7",
    "stripe": "^19.2.0",
    "svix": "^1.81.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.30.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bull": "^3.15.9",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "prisma": "^6.18.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  }
