{
  "buildCommand": "next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "devCommand": "next dev",
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "app/**/*.func.ts": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
